# Required: must be 32+ characters
FINOS_MCP_CACHE_SECRET=replace-with-a-strong-random-secret-at-least-32-chars

# Runtime transport (HTTP for local/cloud deployment)
FINOS_MCP_MCP_TRANSPORT=http
FINOS_MCP_MCP_HOST=0.0.0.0
FINOS_MCP_MCP_PORT=8000

# Optional: increases GitHub API rate limits
FINOS_MCP_GITHUB_TOKEN=

# Optional
FINOS_MCP_LOG_LEVEL=INFO
